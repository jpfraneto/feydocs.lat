igner();\\n  const [formData, setFormData] = useState({\\n    name: &#x27;&#x27;,\\n    symbol: &#x27;&#x27;,\\n    devBuyAmount: &#x27;0&#x27;,\\n  });\\n  const [deploying, setDeploying] = useState(false);\\n  const [result, setResult] = useState(null);\\n \\n  const handleSubmit = async (e) =&gt; {\\n    e.preventDefault();\\n    if (!signer) return;\\n \\n    setDeploying(true);\\n    try {\\n      const service = new FeyDeploymentService(signer);\\n      const config = service.createBasicConfig({\\n        ...formData,\\n        rewardRecipient: await signer.getAddress(),\\n      });\\n \\n      const result = await service.deployToken(config, {\\n        onProgress: (step) =&gt; console.log(step),\\n      });\\n \\n      setResult(result);\\n    } catch (error) {\\n      console.error(&#x27;Deployment failed:&#x27;, error);\\n    } finally {\\n      setDeploying(false);\\n    }\\n  };\\n \\n  return (\\n    &lt;form onSubmit={handleSubmit} className=&quot;deployment-form&quot;&gt;\\n      &lt;div&gt;\\n        &lt;label&gt;Token Name:&lt;/label&gt;\\n        &lt;input\\n          value={formData.name}\\n          onChange={(e) =&gt; setFormData({...formData, name: e.target.value})}\\n          required\\n        /&gt;\\n      &lt;/div&gt;\\n      \\n      &lt;div&gt;\\n        &lt;label&gt;Symbol:&lt;/label&gt;\\n        &lt;input\\n          value={formData.symbol}\\n          onChange={(e) =&gt; setFormData({...formData, symbol: e.target.value})}\\n          required\\n        /&gt;\\n      &lt;/div&gt;\\n      \\n      &lt;div&gt;\\n        &lt;label&gt;Dev Buy Amount (ETH):&lt;/label&gt;\\n        &lt;input\\n          type=&quot;number&quot;\\n          step=&quot;0.01&quot;\\n          value={formData.devBuyAmount}\\n          onChange={(e) =&gt; setFormData({...formData, devBuyAmount: e.target.value})}\\n        /&gt;\\n      &lt;/div&gt;\\n \\n      &lt;button type=&quot;submit&quot; disabled={deploying || !signer}&gt;\\n        {deploying ? &#x27;Deploying...&#x27; : &#x27;Deploy Token&#x27;}\\n      &lt;/button&gt;\\n \\n      {result &amp;&amp; (\\n        &lt;div className=&quot;deployment-result&quot;&gt;\\n          &lt;h3&gt;Deployment Successful!&lt;/h3&gt;\\n          &lt;p&gt;&lt;strong&gt;Token Address:&lt;/strong&gt; {result.tokenAddress}&lt;/p&gt;\\n          &lt;p&gt;&lt;strong&gt;Transaction:&lt;/strong&gt; {result.transactionHash}&lt;/p&gt;\\n        &lt;/div&gt;\\n      )}\\n    &lt;/form&gt;\\n  );\\n}\\n\",\"title\":\"Component Examples\",\"titles\":[\"Integration Guide\",\"React Integration\"]},{\"href\":\"/guides/integration#testing-integration\",\"html\":\"\\n\",\"id\":\"docs/pages/guides/integration.mdx#testing-integration\",\"isPage\":false,\"text\":\"\\n\",\"title\":\"Testing Integration\",\"titles\":[\"Integration Guide\"]},{\"href\":\"/guides/integration#unit-tests\",\"html\":\"\\n<pre class=\\\"shiki shiki-themes github-light github-dark-dimmed\\\" style=\\\"background-color:#fff;--shiki-dark-bg:#22272e;color:#24292e;--shiki-dark:#adbac7\\\" tabindex=\\\"0\\\"><code><span class=\\\"line\\\"><span style=\\\"color:#6A737D;--shiki-dark:#768390\\\">// tests/integration.test.ts</span></span>\\n<span class=\\\"line\\\" data-empty-line=\\\"true\\\"> </span>\\n<span class=\\\"line\\\" data-empty-line=\\\"true\\\"> </span>\\n<span class=\\\"line\\\" data-empty-line=\\\"true\\\"> </span>\\n<span class=\\\"line\\\" data-empty-line=\\\"true\\\"> </span>\\n<span class=\\\"line\\\"><span style=\\\"color:#6F42C1;--shiki-dark:#DCBDFB\\\">describe</span><span style=\\\"color:#24292E;--shiki-dark:#ADBAC7\\\">(</span><span style=\\\"color:#032F62;--shiki-dark:#96D0FF\\\">&#x27;FEY Protocol Integration&#x27;</span><span style=\\\"color:#24292E;--shiki-dark:#ADBAC7\\\">, () </span><span style=\\\"color:#D73A49;--shiki-dark:#F47067\\\">=&gt;</span><span style=\\\"color:#24292E;--shiki-dark:#ADBAC7\\\"> {</span></span>\\n<span class=\\\"line\\\"><span style=\\\"color:#D73A49;--shiki-dark:#F47067\\\">  let</s